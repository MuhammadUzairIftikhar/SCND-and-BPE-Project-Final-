<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from trackingDetailsService WSDL service -->

  <service name="trackingDetails" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://main"/>
      <parameter name="wsdlServiceElement" value="trackingDetailsService"/>
      <parameter name="schemaQualified" value="http://main"/>
      <parameter name="wsdlServicePort" value="trackingDetails"/>
      <parameter name="className" value="main.TrackingDetailsSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="trackingDetails"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="createTrackingDetails" qname="operNS:createTrackingDetails" xmlns:operNS="http://main" soapAction="" >
        <parameter qname="pns:order" xmlns:pns="http://main" type="tns:TrackingDetailsObject" xmlns:tns="http://main"/>
      </operation>
      <operation name="getAllTrackingDetails" qname="operNS:getAllTrackingDetails" xmlns:operNS="http://main" returnQName="retNS:getAllTrackingDetailsReturn" xmlns:retNS="http://main" returnType="rtns:TrackingDetailsObject" xmlns:rtns="http://main" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getAllTrackingDetails createTrackingDetails"/>

      <typeMapping
        xmlns:ns="http://main"
        qname="ns:TrackingDetailsObject"
        type="java:main.TrackingDetailsObject"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
